{
  "name": "@baasix/types",
  "version": "1.0.2",
  "description": "Shared TypeScript types for Baasix packages",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "baasix",
    "types",
    "typescript"
  ],
  "author": "Baasix",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
